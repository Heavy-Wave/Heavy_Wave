{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "@vue/standard"
  ],
  "parserOptions": {
    "parser": "@babel/eslint-parser"
  },
  "rules": {
    "vue/multi-word-component-names": "off",
    "vue/no-mutating-props": "warn",
    "no-console": "warn",
    "no-debugger": "warn"
  }
}
